func fourSum(_ nums: [Int], _ target: Int) -> [[Int]] {
    var res = [[Int]]()
    var sorted = nums
    sorted.sort()
    for k in 0 ..< sorted.count {
        // è¿™ç§å‰ªæä¸è¡Œ,targetå¯èƒ½æ˜¯è´Ÿæ•°
